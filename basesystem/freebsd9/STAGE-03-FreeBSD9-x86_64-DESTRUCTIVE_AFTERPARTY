/usr/share/man/man8/wpa_supplicant.8.gz
/var/games
/var/at
/media
/.cshrc
/.profile
"

for i in ${RMLIST}; do
   echo "Removing: $i"
   ${CHFLAGS_BIN} noschg "${i}"
   ${RM_BIN} -rf "${i}"
done

echo "Linking Linux fallback commands"
${LN_BIN} -s /usr/bin/lockf /usr/bin/flock
${LN_BIN} -s /bin/sh /bin/bash

echo "Moving rescue system"
${MV_BIN} /rescue /usr/rescue > /dev/null

echo "Moving additional utilities to /usr/rescue"
${MV_BIN} /usr/bin/su /usr/rescue/su
${MV_BIN} /usr/bin/w /usr/rescue/w
${MV_BIN} /usr/bin/who /usr/rescue/who

echo "Preparing passwd & group"
${PW_BIN} usermod root -d /usr/root
${MKDIR_BIN} -p /usr/root/.ssh
echo "${SSH_AUTH_KEYS}" > /usr/root/.ssh/authorized_keys

echo "root:*:0:0:Charlie &:/usr/root:/bin/sh
nobody:*:1:1:Unprivileged user:/nonexistent:/usr/sbin/nologin
" > /etc/passwd

echo "wheel:*:0:root
nobody:*:1:
nogroup:*:2:
audit:*:3:
proxy:*:4:
authpf:*:5:
tty:*:6:
operator:*:7:
games:*:8:
bind:*:9:
kmem:*:10:
daemon:*:11:
" > /etc/group

echo "Generating master.passwd"
${PWD_MKDB} /etc/master.passwd


echo "Setting up sshd"

echo "
Host verknowsys.com *.verknowsys.com
Port 60022
" > /etc/ssh/ssh_config

echo "Port 60022
UsePAM no
X11Forwarding no
PrintMotd yes
PrintLastLog yes
TCPKeepAlive yes
UseLogin yes
PermitUserEnvironment yes
Compression delayed
ClientAliveInterval 60
ClientAliveCountMax 3
UseDNS no
RSAAuthentication yes
PubkeyAuthentication yes
PasswordAuthentication yes
PermitRootLogin yes
Subsystem	sftp	/usr/libexec/sftp-server
" > /etc/ssh/sshd_config

echo "Updating login db…"
${MKDB_CAP_BIN} /etc/login.conf

echo "Updating services db…"
${MKDB_SERVICES_BIN} -q -o /var/db/services.db /etc/services

echo "Restarting services"
${SERVICE_BIN} sshd restart
