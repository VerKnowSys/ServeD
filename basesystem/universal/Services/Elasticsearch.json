{
    "softwareName": "Elasticsearch",

    "install": {
        "commands": "sofin get elasticsearch",
        "expectOutput": "All done"
    },
    "start": {
        "commands": "SERVICE_ROOT/exports/elasticsearch -p SERVICE_PREFIX/service.pid -f -Xmx1g -Des.http.port=SERVICE_PORT -Des.index.storage.type=memory > SERVICE_PREFIX/service.log 2>&1 &"
    }
}
