{
    "name": "Memcached",
    "install": {
        "commands": ["sofin get memcached"],
        "expectStdOut": ["Preparing application: Memcached", "All done."],
        "expectOutputTimeout": 256
    },
    "start": {
        "commands": ["SERVICE_ROOT/exports/memcached -p SERVICE_PORT -P SERVICE_PREFIX/service.pid -d"]
    },
    "stop": {
        "commands": ["kill $(cat SERVICE_PREFIX/service.pid)"]
    },
    "afterStop": {
        "commands": ["rm -f SERVICE_PREFIX/service.pid"]
    }
}