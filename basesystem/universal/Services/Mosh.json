{
    "softwareName": "Mosh",
    "install": {
        "commands": ["sofin get mosh"],
        "expectStdOut": ["All done."]
    },
    "start": {
        "commands": ["SERVICE_ROOT/exports/mosh-server new -- /Software/Zsh/exports/zsh -s"]
    },
    "stop": {
        "commands": ["rm -f SERVICE_PREFIX/client.string.txt"]
    }
}