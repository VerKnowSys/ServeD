- import com.verknowsys.served.utils.Dict
- import com.verknowsys.served.web.merch._
- include("layout.jade")

h1= Dict("Product list")
h3
  a(href="/product")= Dict("Add new product")

.tshirts
  - if (clothings.length == 0)
    p= Dict("No clothing available")

  - for (cloth <- clothings)
    p= cloth

.stickers
  - for (sticker <- stickers)
    p= sticker

.letters
  - for (letter <- letters)
    p= letter

