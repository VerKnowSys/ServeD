<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
  version="2.4">

  <filter>
    <filter-name>LoggerFilter</filter-name>
    <filter-class>com.verknowsys.served.web.filters.LoggerFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>LoggerFilter</filter-name>
    <url-pattern>/logger/*</url-pattern>
  </filter-mapping>

  <filter>
    <filter-name>GitFilter</filter-name>
    <filter-class>com.verknowsys.served.web.filters.GitFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>GitFilter</filter-name>
    <url-pattern>/git/*</url-pattern>
  </filter-mapping>

  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>/images/*</url-pattern>
    <url-pattern>/css/*</url-pattern>
    <url-pattern>/js/*</url-pattern>
  </servlet-mapping>
</web-app>